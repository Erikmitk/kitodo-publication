<t3datastructure>
    <meta>
        <langdisable>
            1
        </langdisable>
    </meta>
    <sheets>
        <sdef>
            <root>
                <tceforms>
                    <sheettitle>
                        LLL:EXT:dpf/Resources/Private/Language/locallang.xlf:tx_dpf_qucosaform.options
                    </sheettitle>
                </tceforms>
                <type>
                    array
                </type>
                <el>
                    <switchablecontrolleractions>
                        <tceforms>
                            <label>
                                LLL:EXT:dpf/Resources/Private/Language/locallang.xlf:tt_content.pi_flexform.select_function
                            </label>
                            <onchange>
                                reload
                            </onchange>
                            <config>
                                <type>
                                    select
                                </type>
                                <items>
                                    <numindex index="0">
                                        <numindex index="0">
                                            LLL:EXT:dpf/Resources/Private/Language/locallang.xlf:tt_content.pi_flexform.searchAndResults
                                        </numindex>
                                        <numindex index="1">
                                            SearchFE->search;SearchFE->list;SearchFE->nextResults
                                        </numindex>
                                    </numindex>
                                    <numindex index="1">
                                        <numindex index="0">
                                            LLL:EXT:dpf/Resources/Private/Language/locallang.xlf:tt_content.pi_flexform.showSearchForm
                                        </numindex>
                                        <numindex index="1">
                                            SearchFE->showSearchForm
                                        </numindex>
                                    </numindex>
                                </items>
                            </config>
                        </tceforms>
                    </switchablecontrolleractions>
                    <settings.landingpage>
                        <tceforms>
                            <exclude>
                                1
                            </exclude>
                            <label>
                                LLL:EXT:dpf/Resources/Private/Language/locallang.xlf:tt_content.pi_flexform.landingPage
                            </label>
                            <config>
                                <type>
                                    group
                                </type>
                                <internal_type>
                                    db
                                </internal_type>
                                <allowed>
                                    pages
                                </allowed>
                                <size>
                                    1
                                </size>
                                <maxitems>
                                    1
                                </maxitems>
                                <minitems>
                                    0
                                </minitems>
                                <wizards>
                                    <suggest>
                                        <type>
                                            suggest
                                        </type>
                                    </suggest>
                                </wizards>
                            </config>
                        </tceforms>
                    </settings.landingpage>
                    <settings.searchresultspid>
                        <tceforms>
                            <exclude>
                                1
                            </exclude>
                            <label>
                                LLL:EXT:dpf/Resources/Private/Language/locallang.xlf:tt_content.pi_flexform.searchResultsPid
                            </label>
                            <config>
                                <type>
                                    group
                                </type>
                                <internal_type>
                                    db
                                </internal_type>
                                <allowed>
                                    pages
                                </allowed>
                                <size>
                                    1
                                </size>
                                <maxitems>
                                    1
                                </maxitems>
                                <minitems>
                                    0
                                </minitems>
                                <wizards>
                                    <suggest>
                                        <type>
                                            suggest
                                        </type>
                                    </suggest>
                                </wizards>
                            </config>
                        </tceforms>
                    </settings.searchresultspid>
                    <settings.apipid>
                        <tceforms>
                            <exclude>
                                1
                            </exclude>
                            <label>
                                LLL:EXT:dpf/Resources/Private/Language/locallang.xlf:tt_content.pi_flexform.apiPid
                            </label>
                            <config>
                                <type>
                                    group
                                </type>
                                <internal_type>
                                    db
                                </internal_type>
                                <allowed>
                                    pages
                                </allowed>
                                <size>
                                    1
                                </size>
                                <maxitems>
                                    1
                                </maxitems>
                                <minitems>
                                    0
                                </minitems>
                                <wizards>
                                    <suggest>
                                        <type>
                                            suggest
                                        </type>
                                    </suggest>
                                </wizards>
                            </config>
                        </tceforms>
                    </settings.apipid>
                </el>
            </root>
        </sdef>
    </sheets>
</t3datastructure>
